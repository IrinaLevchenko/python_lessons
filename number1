print('Hello!')

my_name = 'Irina'
my_age = 30

print('My name is', my_name,', I am', my_age, 'years old.')

your_name = input('Print your name: ')
your_age = input('How old are you? ')
your_age = int(your_age)

if(your_age >= my_age):
    print('Goodbye!')
else:
    print('Hey you!')

print('Nice talking to you,', your_name, '!', 'Life is good, even at', your_age, '!')

